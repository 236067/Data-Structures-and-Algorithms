class Solution {
        public void reverseString(char[] s) {
            int p1=0;
            int p2=s.length-1;
            char temp='0';
            while(p1<p2){
                temp=s[p1];
                s[p1]=s[p2];
                s[p2]=temp;
                p1++;
                p2--;
            }
        }
    }
